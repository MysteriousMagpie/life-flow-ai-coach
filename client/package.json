{
  "name": "life-flow-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
"scripts": {
  "dev": "vite",
  "build": "tsc && vite build",
  "preview": "vite preview",
  "test:e2e": "NODE_OPTIONS='--loader ts-node/esm' playwright test",
  "test:e2e:ui": "NODE_OPTIONS='--loader ts-node/esm' playwright test --ui",
  "test:e2e:headed": "NODE_OPTIONS='--loader ts-node/esm' playwright test --headed",
  "test:e2e:debug": "NODE_OPTIONS='--loader ts-node/esm' playwright test --debug",
  "test:e2e:report": "playwright show-report",
  "test:e2e:organize": "ts-node e2e/utils/screenshot-organizer.ts",
  "test:e2e:summary": "ts-node e2e/reporters/github-summary.ts test-results/reports",
  "test:auth": "NODE_OPTIONS='--loader ts-node/esm' playwright test auth.spec.ts",
  "test:ai": "NODE_OPTIONS='--loader ts-node/esm' playwright test ai-coaching.spec.ts",
  "test:dashboard": "NODE_OPTIONS='--loader ts-node/esm' playwright test dashboard.spec.ts",
  "test:profile": "NODE_OPTIONS='--loader ts-node/esm' playwright test profile-settings.spec.ts",
  "test:integration": "NODE_OPTIONS='--loader ts-node/esm' playwright test integration.spec.ts",
  "test:install": "playwright install",
  "test:ci": "NODE_OPTIONS='--loader ts-node/esm' playwright test --reporter=html,json,junit && pnpm test:e2e:summary"
},
